package com.gh.greenhouse.domain;
/**
 * 作物信息
 * @author 吴奇俊
 * 2015-10-18 下午6:57:25
 */
public class Crop {
	
/**
 * 主键id
*/
private Integer Crop_id;

/**
 * 作物名称
 */
private String Crop_name;

/**
 *作物编码 
 *作物品种代号，两位数"01","02","03"
 */
private Integer Crop_code;

/**
 * 作物品种
 */
private String Crop_type;

/**
 * 作物栽培要求
 */
private String Crop_period;
/**
 * 是否被删除
 */
private String deleted;

public String getDeleted() {
	return deleted;
}

public void setDeleted(String deleted) {
	this.deleted = deleted;
}


public Integer getCrop_id() {
	return Crop_id;
}
public void setCrop_id(Integer crop_id) {
	Crop_id = crop_id;
}
public String getCrop_name() {
	return Crop_name;
}
public void setCrop_name(String crop_name) {
	Crop_name = crop_name;
}
public Integer getCrop_code() {
	return Crop_code;
}
public void setCrop_code(Integer crop_code) {
	Crop_code = crop_code;
}
public String getCrop_type() {
	return Crop_type;
}
public void setCrop_type(String crop_type) {
	Crop_type = crop_type;
}
public String getCrop_period() {
	return Crop_period;
}
public void setCrop_period(String crop_period) {
	Crop_period = crop_period;
}

}
