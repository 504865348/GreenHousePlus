.navBar{
	position: relative;
	z-index: 20;
	margin: 0px;
	padding: 0px;
	box-shadow: 0px 0px 3px #CCC;
	background-color:#5C6A85;
}
.navBar .navInner{
	background: transparent -moz-linear-gradient(center bottom , #4D5B76 0%, #6C7A95 100%) repeat scroll 0% 0% padding-box;
	padding: 10px;
	margin: 0px;
	border-radius: 0px;
	border-width: 0px 0px medium;
	border-style: none;
	border-color: -moz-use-text-color;
	-moz-border-top-colors: none;
	-moz-border-right-colors: none;
	-moz-border-bottom-colors: none;
	-moz-border-left-colors: none;
	border-image: none;
	min-height: 40px;
}
.navInner::before, .navbar-inner::after {
    display: table;
    content: "";
    line-height: 0;
}
.topTitle{
	font-size:18px;
	color:#fff;
}
.topName a span{
	color:#fff;
}
.navInner > .container > .row > div > span > a{
	display:inline-block;
	font-size:14px;
	width:100%;
	color:#fff;
	text-align:center;
}
.navInner > .container > .row > div > span > a:hover{
	color:#000;
}
.topName,
.topLogin,
.sysnews{
	display:inline-block;
	font-size:14px;
	color:#fff;
}
/************************************侧栏********************************/
.sidebar-nav {
    width: 15%;
    position: absolute;
    float: left;
    border-top: medium none;
    border-left: medium none;
    margin: 0em;
    border-right:3px solid #5C6A85;
}
.sidebar-nav .nav-header {
    border-top: 1px solid #FFF;
    border-bottom: 1px solid #C8C8CB;
    border-left: medium none;
    color: #fff;
    display: block;
   background:#5C6A85;
    font-weight: normal;
    font-size: 1em;
    line-height: 2.5em;
    padding: 0em 0.25em;
    margin-bottom: 0px;
    text-shadow: none;
    text-transform: none;
    text-align:center;
}
.sidebar-nav a.nav-header:hover{
	background:#5C6A85;
	color:#333;
} 
/********************* background:#5C6A85;
    background: -webkit-gradient(linear, left bottom, left top, color-stop(0, #5C6A85), color-stop(1, #ffffff));
    background: -ms-linear-gradient(bottom, #5C6A85, #ffffff);
    background: -moz-linear-gradient(center bottom, #5C6A85 0%, #ffffff 100%);
    background: -o-linear-gradient(bottom, #5C6A85, #ffffff);
    filter: progid:dximagetransform.microsoft.gradient(startColorStr='#4d5b76', EndColorStr='#6c7a95');
    -ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorStr='#ffffff',EndColorStr='#5C6A85')";
    ************处理渐变*/
.sidebar-nav .nav-list.collapse{
    border-bottom: 1px solid #CCC;
}
.sidebar-nav .nav-list {
    margin: 0px;
    border: 0px none;
    background: #f6f6f6 none repeat scroll 0% 0%;
    text-align:center;
}
.nav-list {
	padding-left: 15px;
	padding-right: 15px;
	margin-bottom: 0px;
}
/****************************向下小箭头************************/
.sidebar-nav .nav-header i[class^="icon-"] {
    margin-right: 0.75em;
}
.sidebar-nav .nav-header .icon-chevron-up {
    float: right;
}
a [class^="icon-"], a [class*=" icon-"] {
    display: inline-block;
    text-decoration: inherit;
}
.iconSpan{
	display:inline-block;
	margin-right:5px;
}
/***********************************************************/
.tableTitle{
	width:500px;
	text-align:center;
}
/**************************mainIframe*******************/
.rightContent{
	width:85%;
	height:auto;
}
.mainIframe{
	border:none;
	width:100%;
	height:1000px;
	background:#fff;
}
.h3{
	text-align:center;
	font-size:18px;
}